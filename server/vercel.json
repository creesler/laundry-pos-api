{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/admin/:path*",
      "destination": "/server.js"
    },
    {
      "source": "/api/:path*",
      "destination": "/server.js"
    },
    {
      "source": "/:path*",
      "destination": "/server.js"
    }
  ]
}